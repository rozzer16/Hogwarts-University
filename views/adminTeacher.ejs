<!--Main Start-->
<main>
<h1>Teacher</h1>

<div class="body" style="margin-bottom: 320px;">
  <form action="/Teacher-Admin" method="post">
  <div class="container">
      <div class="row">

      <div class="col-lg-4 col-md-5 col-sm-6 justify-content-start" id="left">
          <div class="form-group" style="margin-bottom: 25px;">
              <label for="name">Name *</label>
              <input type="name" class="form-control shadow rounded" id="name" placeholder="Teacher name"  name="name" required>
          </div>
          <div class="form-group" style="margin-bottom: 25px;">
              <label for="name">Email *</label>
              <input type="name" class="form-control shadow rounded" id="email" placeholder="Email"  name="email" required>
          </div>

          <div class="form-group" style="margin-bottom: 25px;">
              <label for="course">Specialization *</label>
              <select class="form-control form-control-sm shadow rounded" name="Specialization" id="Specialization" required>
                  <option value="#" name="None">Select</option>
                  <option value="Probability">Probability</option>
                  <option value="Software Engineering">Software Engineering</option>
                  <option value="Python">Python</option>
                  <option value="Sift Skills">Sift Skills</option>
                  <option value="Microprocessor">Microprocessor</option>
                  <option value="Computer Network">Computer Network</option>
                  <option value="Data Structure">Data Structure</option>




              </select>

              <div class="form-group" style="margin-bottom: 25px;margin-top: 20px;">
                  <label for="name">Address *</label>
                  <input type="name" class="form-control shadow rounded" id="address" placeholder="Address"  name="address" required>
              </div>
          </div>


      </div>

      <div class="col-lg-4 col-md-5 col-sm-6 justify-content-center" id="center">
          <div class="form-group" style="margin-bottom: 25px;">
              <label for="uid">EID *</label>
              <input type="text" class="form-control shadow rounded" id="eid" placeholder="eg: 21CS001" name="eid" required>
          </div>
          <div class="form-group">
              <label for="password">Password *</label>
              <input type="text" class="form-control shadow rounded" id="password" placeholder="eg: John001" name="password" required>
          </div>
          <div class="form-group">
              <label for="year">Admission Year *</label>
              <input type="year" class="form-control shadow rounded" id="joiningyear" placeholder="eg: 2021" name="joiningyear" required>
          </div>




      </div>

      <div class="col-lg-4 col-md-5 col-sm-6 justify-content-end" id="right">
          <div class="form-group" style="margin-bottom: 25px;">
              <label for="dob">D.O.B *</label>
              <input type="date" class="form-control shadow rounded" id="dob" name="dob" required>
          </div>
          <div class="form-group" style="margin-bottom: 25px;">
              <label for="name">Number *</label>
              <input type="name" class="form-control shadow rounded" id="phone" placeholder="number"  name="phone" required>
          </div>
          <div class="form-group" style="margin-bottom: 25px;">
              <label for="gender">Gender *</label>
              <select class="form-control form-control-sm shadow rounded" name="gender" required>
                  <option value="#">Select</option>
                  <option value="Male">Male</option>
                  <option value="Female">Female</option>
                  <option value="Others">Others</option>
                </select>



          </div>

                </select>
          </div>
          <div class="toast">
            <div class="toast-content">
                <i class="fas fa-solid fa-check check"></i>

                <div class="message">
                    <span class="text text-1">Success</span>
                    <span class="text text-2">Your changes has been saved</span>
                </div>
            </div>
            <i class="fa-solid fa-xmark close"></i>

            <div class="progress"></div>
          </div>

          <div class="form-group buttonBox" style="margin-left: 500px;margin-top: 30px;";>
              <button type="submit" class="btn btn-dark">Submit</button>
          </div>
      </div>

</div>

</div>

</form>

