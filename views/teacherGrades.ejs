    <div class="container marks">
        <h2>Python</h2>
    </div>

    <div class="container marks-academics">
      <table class="table table-light table-hover">
            <thead class="thead-dark">
              <tr>
                <th scope="col" id="round-left">#</th>
                <th scope="col" id="middle">Name</th>
                <th scope="col" id="middle">Marks</th>
                <th scope="col" id="middle">Update</th>
                <th scope="col" id="round-right">Submit</th>
              </tr>
            </thead>
        <tbody>
          <tr>
            <%for(var i=0;i<Student.length;i++){%>
            <%var count = i+1;%>
            <th scope="row"><%=count%></th>
            <td><%=Student[i].name%></td>
            <form action="/python" method="post">
              <td><p style="padding-top: 20px;margin-bottom: 0px;"><%=Student[i].marks[2]%></p></td>
              <input type="hidden" value="<%=Student[i].uid%>" name="uid">
              <input type="hidden" value="<%=Teacher.eid%>" name="eid">
              <td><input type="text" placeholder="Marks" id="marks" name="marks" ></td>
              <td><button type="submit" class="btn btn-dark" style="width: 50px;" value="<%=i%>"><i class="fas fa-check-circle"></i></button></td>
            </form>
          </tr>
          <%}%>
        </tbody>
      </table>
    </div>

