<!--Main Start-->
<main>

<h1>Profile</h1>

<table cellspacing = "20">
  <tr>
  <td><img src="./images/ppl.png" alt="Atanu's photo not found" width="100" height="100" style="border-left-width: 500px;margin-left: 470px;"></td>
  </tr>
</table>


<div class="container">
  <div class="row">


  <div class="main-body">

          <div class="col-md-8">
            <div class="card mb-3">
              <div class="card-body">
                <div class="row">
                  <div class="col-sm-3">
                    <h4 class="mb-0">Teacher Info</h4>
                  </div>
                  <br>
                  <div class="col-sm-3">
                    <h5 class="mb-0">Full Name</h5>
                  </div>
                  <div class="col-sm-9 text-secondary">
                    <%=Teacher.name%>
                  </div>
                </div>

                <div class="row">
                  
                  <div class="col-sm-3">
                    <h5 class="mb-0">Email</h5>
                  </div>
                  <div class="col-sm-9 text-secondary">
                    <%=Teacher.email%>
                  </div>
                </div>

                <div class="row">
                  <div class="col-sm-3">
                    <h5 class="mb-0">Phone</h5>
                  </div>
                  <div class="col-sm-9 text-secondary">
                    +91 <%=Teacher.email%> 
                  </div>
                </div>

              

                <div class="row">
                  <div class="col-sm-3">
                    <h5 class="mb-0">Address</h5>
                  </div>
                  <div class="col-sm-9 text-secondary">
                    <%=Teacher.address%>
                  </div>
                </div>


              </div>
            </div>
  <!-- Academic-->
            <div class="row gutters-sm">

              <div class="col-sm-6 mb-3">
                <div class="card mb-3">
                  <div class="card-body">
                    <div class="row">
                      <div class="col-sm-3">
                        <h4 class="mb-0">Academic</h4>
                      </div>
                      <br>
                    </div>

                    <div class="row">
                      <div class="col-sm-3">
                        <h5 class="mb-0">specialization</h5>
                      </div>
                      <div class="col-sm-9 text-secondary">
                        <%=Teacher.spelization%>
                      </div>
                    </div>

                    <div class="row">
                      <div class="col-sm-3">
                        <h5 class="mb-0">
                          experience</h5>
                      </div>
                      <div class="col-sm-9 text-secondary">
                        <%=Teacher.joiningyear%>-2022
                      </div>
                    </div>

                   


                  </div>
                </div>
              </div>
            </div>
       <!--CGP-->
            


          </div>
        </div>

      </div>
    </div>
  </div>








  <!--footer-->

  <p class="copyright">
    &copy; 2022 - <span>Hogwarts University</span> All Rights Reserved.
  </p>
</main>

  <script src="js/dashboard.js"></script>

  <script>

    var preloader = document.getElementById("Loading");
  

    function myfung(){
      preloader.style.display = 'none';
    };
    </script>



  </body>
</html>
